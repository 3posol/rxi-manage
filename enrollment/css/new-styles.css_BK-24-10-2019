/* Create a custom radio button */

.rb-checkmark {
	border-radius: 50%;
}

/* On mouse-over, add a grey background color */
.rb-container:hover input ~ .rb-checkmark, .cb-container:hover input ~ .cb-checkmark {
	background-color: #6dacde;
}

/* When the radio button is checked, add a blue background */
.rb-container input:checked ~ .rb-checkmark, .cb-container input:checked ~ .cb-checkmark {
  	background-color: #6dacde;
}

/* Create the indicator (the dot/circle - hidden when not checked) */
.rb-checkmark:after, .cb-checkmark:after {
    content: "";
    position: absolute;
    display: none;
}

/* Show the indicator (dot/circle) when checked */
.rb-container input:checked ~ .rb-checkmark:after, .cb-container input:checked ~ .cb-checkmark:after {
    display: block;
}

